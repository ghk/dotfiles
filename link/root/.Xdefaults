!-------------------------------------------------------------------------------
! Xft settings
!-------------------------------------------------------------------------------
 
Xft.dpi: 96
Xft.autohint: 0
Xft.hinting: true
Xft.rgba: rgb
Xft.lcdfilter: lcddefault
Xft.antialias: 1
Xft.hintstyle: hintfull
 
!-------------------------------------------------------------------------------
! URxvt settings
! Colours lifted from Solarized (http://ethanschoonover.com/solarized)
! More info at:
! http://pod.tst.eu/http://cvs.schmorp.de/rxvt-unicode/doc/rxvt.1.pod
!-------------------------------------------------------------------------------
 
URxvt.depth: 256
URxvt.meta8: false
!URxvt.geometry: 90x30
URxvt.transparent: false
URxvt.fading: 0
! URxvt.urgentOnBell: true
URxvt.visualBell: true
URxvt.loginShell: true
URxvt.saveLines: 20000
!URxvt.internalBorder: 3
URxvt.lineSpace: 0
 
! Fonts
URxvt.allow_bold: false
/* URxvt.font: -*-terminus-medium-r-normal-*-12-120-72-72-c-60-iso8859-1 */
URxvt*font: xft:Consolas\ for\ Powerline:pixelsize=13:style=Regular:antialias=false
URxvt*boldFont: xft:Consolas\ for\ Powerline:pixelsize=13:style=Regular:antialias=false
 
! Fix font space
!URxvt*letterSpace: -1
 
! Scrollbar
!URxvt.scrollStyle: rxvt
URxvt.scrollBar: false
URxvt.internalBorder: 0
!URxvt.externalBorder: 1
URxvt.borderColor: #1c1c1c
!URxvt.depth: 32
!URxvt*borderless: true

! Perl extensions
URxvt.perl-ext-common: default,matcher,clipboard
URxvt.keysym.M-c:   perl:clipboard:copy
URxvt.keysym.M-v:   perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
URxvt.keysym.C-i: 
URxvt.matcher.button: 1
URxvt.urlLauncher: firefox
 
! Cursor
URxvt.cursorBlink: true
URxvt.cursorColor: #657b83
URxvt.cursorUnderline: false
 
! Pointer
URxvt.pointerBlank: true
 
